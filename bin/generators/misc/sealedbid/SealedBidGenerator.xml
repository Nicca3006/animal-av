<?xml version="1.0" encoding="ISO-8859-1"?>
<PropertiesTreeModel
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="PropertiesTreeModel.xsd">
	<Folder name="Root">
		<Primitive type="StringArray">
			<name>items</name>
			<value>
				<StringArray>Fahrrad, Koffer, Armbanduhr</StringArray>
			</value>
			<description>Items that are auctioned. There should be at least one
				element to auction.
			</description>
		</Primitive>
		<Primitive type="intMatrix">
			<name>playerBids</name>
			<value>
				<intMatrix>420, 60, 180; 300, 75, 150; 350, 60, 100</intMatrix>
			</value>
			<description>Bids of each player for each auctioned item. Each row
				represents a player, each column represents an item. CAUTION: Number
				of columns should match number of defined items in 'items'
				primitive. Number of players should at least be 2. There should be
				no negative bids.
			</description>
		</Primitive>
		<Primitive type="boolean">
			<name>useQuestions</name>
			<value>
				<boolean value="true" />
			</value>
			<description>Decides whether or not multiple choice questions will be
				used throughout the animation.
			</description>
		</Primitive>
		<AnimationProperties type="MatrixProperties">
			<AnimationPropertyItem type="ColorPropertyItem">
				<name>cellHighlight</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<Color>(0, 0, 0)</Color>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem type="ColorPropertyItem">
				<name>borderColor</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<Color>(0, 0, 0)</Color>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem type="ColorPropertyItem">
				<name>color</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<Color>(0, 0, 0)</Color>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="BooleanPropertyItem">
				<name>hidden</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<boolean value="false" />
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="BooleanPropertyItem">
				<name>filled</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<boolean value="false" />
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="EnumerationPropertyItem">
				<name>align</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<Enumeration>left, left, center, right</Enumeration>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem type="ColorPropertyItem">
				<name>elementColor</name>
				<isEditable value="true" />
				<label>Color of matrix elements</label>
				<value>
					<Color>(0, 0, 100)</Color>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem type="ColorPropertyItem">
				<name>fillColor</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<Color>(0, 0, 0)</Color>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem type="ColorPropertyItem">
				<name>highlightBorderColor</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<Color>(255, 0, 0)</Color>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="IntegerPropertyItem">
				<name>depth</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<int>1</int>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="IntegerPropertyItem">
				<name>cellheight</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<int>-1</int>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="StringPropertyItem">
				<name>name</name>
				<isEditable value="true" />
				<label></label>
				<value>
					<String>matrix</String>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="EnumerationPropertyItem">
				<name>style</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<Enumeration>plain, plain, matrix, table</Enumeration>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="IntegerPropertyItem">
				<name>cellwidth</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<int>-1</int>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem type="FontPropertyItem">
				<name>font</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<Font>SansSerif</Font>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem type="ColorPropertyItem">
				<name>elemHighlight</name>
				<isEditable value="true" />
				<label>Highlight color of matrix elements</label>
				<value>
					<Color>(255, 0, 0)</Color>
				</value>
			</AnimationPropertyItem>
		</AnimationProperties>
		<AnimationProperties type="SourceCodeProperties">
			<AnimationPropertyItem type="ColorPropertyItem">
				<name>highlightColor</name>
				<isEditable value="true" />
				<label>Color of highlighted pseudocode</label>
				<value>
					<Color>(255, 0, 0)</Color>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem type="ColorPropertyItem">
				<name>contextColor</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<Color>(0, 0, 0)</Color>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="IntegerPropertyItem">
				<name>depth</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<int>1</int>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="IntegerPropertyItem">
				<name>indentation</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<int>1</int>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem type="ColorPropertyItem">
				<name>color</name>
				<isEditable value="true" />
				<label>Color of pseudocode text</label>
				<value>
					<Color>(153, 153, 255)</Color>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="BooleanPropertyItem">
				<name>hidden</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<boolean value="false" />
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="IntegerPropertyItem">
				<name>size</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<int>10</int>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="StringPropertyItem">
				<name>name</name>
				<isEditable value="true" />
				<label></label>
				<value>
					<String>sourceCode</String>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="BooleanPropertyItem">
				<name>bold</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<boolean value="false" />
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="IntegerPropertyItem">
				<name>row</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<int>1</int>
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem
				type="BooleanPropertyItem">
				<name>italic</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<boolean value="false" />
				</value>
			</AnimationPropertyItem>
			<AnimationPropertyItem type="FontPropertyItem">
				<name>font</name>
				<isEditable value="false" />
				<label></label>
				<value>
					<Font>SansSerif</Font>
				</value>
			</AnimationPropertyItem>
		</AnimationProperties>
	</Folder>
</PropertiesTreeModel>