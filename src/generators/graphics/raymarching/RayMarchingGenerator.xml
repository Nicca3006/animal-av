					<value><String>Ray</String></value>
				</AnimationPropertyItem>
				<AnimationPropertyItem type="BooleanPropertyItem">
					<name>bwArrow</name>
					<isEditable value="false" />
					<label></label>
					<value><boolean value="false" /></value>
				</AnimationPropertyItem>
				<AnimationPropertyItem type="BooleanPropertyItem">
					<name>fwArrow</name>
					<isEditable value="true" />
					<label>Turn of to deactivate arrows on rays</label>
					<value><boolean value="true" /></value>
				</AnimationPropertyItem>
			</AnimationProperties>
			<Primitive type="Color">
				<name>Ray Circle Color</name>
				<value><Color>(0, 0, 0)</Color></value>
				<description>Color of the circles used to display the distances</description>
			</Primitive>
			<Primitive type="Color">
				<name>Ray Circle Highlight Color</name>
				<value><Color>(255, 0, 0)</Color></value>
				<description>Color of the circle used to display the shortest distance</description>
			</Primitive>
		</Folder>
		<Primitive type="StringMatrix">
			<name>Objects</name>
			<value><StringMatrix>rectangle, 255 255 0, true, 300 300, 60 100, 30; circle, 0 0 255, true, 200 50, 40, null; circle, 0 255 0, true, 600 300, 60, null</StringMatrix></value>
			<description>Description of the objects in the scene</description>
		</Primitive>
		<Primitive type="Color">
			<name>Bounding Color</name>
			<value><Color>(192, 192, 192)</Color></value>
			<description>Color of the bounding box / bounding circle</description>
		</Primitive>
		<Primitive type="double">
			<name>Animation Speed</name>
			<value><double>1.0</double></value>
			<description>Speed of the ray animations (lower is faster)</description>
		</Primitive>
