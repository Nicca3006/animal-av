		<Folder name="Camera">
			<AnimationProperties type="PolygonProperties">
				<AnimationPropertyItem type="ColorPropertyItem">
					<name>fillColor</name>
					<isEditable value="true" />
					<label></label>
					<value><Color>(255, 200, 0)</Color></value>
				</AnimationPropertyItem>
				<AnimationPropertyItem type="IntegerPropertyItem">
					<name>depth</name>
					<isEditable value="false" />
					<label></label>
					<value><int>3</int></value>
				</AnimationPropertyItem>
				<AnimationPropertyItem type="ColorPropertyItem">
					<name>color</name>
					<isEditable value="true" />
					<label></label>
					<value><Color>(0, 0, 0)</Color></value>
				</AnimationPropertyItem>
				<AnimationPropertyItem type="BooleanPropertyItem">
					<name>hidden</name>
					<isEditable value="true" />
					<label></label>
					<value><boolean value="false" /></value>
				</AnimationPropertyItem>
				<AnimationPropertyItem type="StringPropertyItem">
					<name>name</name>
					<isEditable value="true" />
					<label></label>
					<value><String>Camera</String></value>
				</AnimationPropertyItem>
				<AnimationPropertyItem type="BooleanPropertyItem">
					<name>filled</name>
					<isEditable value="true" />
					<label></label>
					<value><boolean value="true" /></value>
				</AnimationPropertyItem>
			</AnimationProperties>
			<Primitive type="intArray">
				<name>Camera Position</name>
				<value><intArray>30, 30</intArray></value>
				<description>Position of the Camera within the Scene</description>
			</Primitive>
			<Primitive type="int">
				<name>Camera Spread</name>
				<value><int>100</int></value>
				<description>The viewing angle of the camera</description>
			</Primitive>
			<Primitive type="int">
				<name>Camera Angle</name>
				<value><int>45</int></value>
				<description>The direction the camera is pointed</description>
			</Primitive>
			<Primitive type="int">
				<name>Number of Rays</name>
				<value><int>20</int></value>
				<description>The number of rays sent out by the camera</description>
			</Primitive>
		</Folder>
		<Folder name="Ray">
			<AnimationProperties type="PolylineProperties">
				<AnimationPropertyItem type="IntegerPropertyItem">
					<name>depth</name>
					<isEditable value="false" />
