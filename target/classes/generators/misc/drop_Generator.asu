%Animal 2 1000*600
title "Drop-Heuristik [DE]"
author "Julian Bonrath, Benjamin Björngen-Schmidt"
{
  interactionDefinition "intDef20120901190812127.txt"
  text "dropCost_into_Header" "Einleitung" (10, 40) color (0, 0, 0) depth 1 font SansSerif size 28 bold
  rectangle "backgroundRectInto" offset (-5, -5) from "dropCost_into_Header" NW offset (7, 5) from "dropCost_into_Header" SE color (0, 0, 0) depth 5 filled  fillColor (128, 128, 128)
  text "dropCost_into_Header2" "Grundidee des Drop-Verfahrens" offset (25, 40) from "dropCost_into_Header" NW color (0, 0, 0) depth 1 font SansSerif size 16 bold
  text "dropCost_introProblem1" "Der Drop-Algorithmus ist eine Heuristik zur Lösung eines diskreten Warehouse Location Problem." offset (10, 20) from "dropCost_into_Header2" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introProblem2" "In diesem Problem geht es um die Fragen welche Lagerhäuser, von einer gegeben" offset (0, 15) from "dropCost_introProblem1" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introProblem3" "Menge an Lagerhäuser weiterhin betrieben werden soll. Das Ziel " offset (0, 30) from "dropCost_introProblem1" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introProblem4" "ist die minimierung von fixen Kosten für das Betreiben von Lagerhäusern sowie" offset (0, 45) from "dropCost_introProblem1" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introProblem5" "den anfallenden Transportkosten von Lagerhäusern zu Kunden." offset (0, 60) from "dropCost_introProblem1" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introIdea1" "Der Drop-Algorithmus geht von der Startsituation aus, dass alle potentiellen" offset (10, 115) from "dropCost_into_Header2" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introIdea2" "Lagerstandorte  vorläufig einbezogen sind. Zu Beginn entspricht der Ziel-" offset (0, 15) from "dropCost_introIdea1" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introIdea3" "funktionswert der Summe der Fixkosten aller Standorte und der minimalen" offset (0, 30) from "dropCost_introIdea1" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introIdea4" "Transportkosten." offset (0, 45) from "dropCost_introIdea1" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introIdea5" "Mit jeder Iteration wird nun derjenige Standort endgultig ausgeschlossen, durch" offset (0, 60) from "dropCost_introIdea1" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introIdea6" "dessen Schließung die größtmögliche Verringerung des Zielfunktionswertes erlangt wird." offset (0, 75) from "dropCost_introIdea1" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introIdea7" "Der Algorithmus endet, sobald durch die Schließung eines weiteren" offset (0, 90) from "dropCost_introIdea1" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introIdea8" "Standortes keine zusatzliche Senkung des Zielfunktionswertes erreicht werden" offset (0, 105) from "dropCost_introIdea1" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introIdea9" "kann. Alle Standorte, die nicht ausgeschlossen wurden, werden nun endgültig" offset (0, 120) from "dropCost_introIdea1" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_introIdea10" "einbezogen. [SCHILDT 1994]" offset (0, 135) from "dropCost_introIdea1" NW color (0, 0, 0) depth 1 font SansSerif size 12
}
label "Einleitung"
{
  text "dropCost_into_Header23" "Der Algorithmus" (10, 40) color (0, 0, 0) depth 1 font SansSerif size 28 bold
  interaction "trueFalseQuestion"
  rectangle "backgroundRectAlgo" offset (-5, -5) from "dropCost_into_Header23" NW offset (7, 5) from "dropCost_into_Header23" SE color (0, 0, 0) depth 5 filled  fillColor (128, 128, 128)
  text "dropCost_Header" "Kosten" offset (25, 50) from "dropCost_into_Header23" NW color (0, 0, 0) depth 1 font SansSerif size 16 bold
  codegroup "sourceCode" at offset (580, -20) from "dropCost_Header" NW color (0, 0, 0) highlightColor (255, 0, 0) contextColor (0, 0, 255) font Monospaced size 12 depth 1
  addCodeLine "Erläuterung:" to "sourceCode" indentation 1
  addCodeLine "Menge aller Lagerhäuser I" to "sourceCode" indentation 2
  addCodeLine "c_i1,j := Transportkosten von Kunde j zum 'nächstgelegenen' Lager i1" to "sourceCode" indentation 2
  addCodeLine "c_i2,j := Transportkosten von Kunde j zu 'übernächstem' Lager i2" to "sourceCode" indentation 2
  addCodeLine "Fülle Tabellen mit Transportkosten und Warenhaus Fixkosten" to "sourceCode" indentation 2
  addCodeLine "" to "sourceCode" indentation 1
  addCodeLine "Iteration:" to "sourceCode" indentation 1
  addCodeLine "a) Minimale Kosten" to "sourceCode" indentation 2
  addCodeLine "Finde minimale Kosten c_i1, c_i2 für alle j und i aus I" to "sourceCode" indentation 2
  addCodeLine "Trage geringste  Kosten in Zeilen c_i1 und " to "sourceCode" indentation 3
  addCodeLine "zweitgeringste Kosten in Zeilen c_i2 ein" to "sourceCode" indentation 3
  addCodeLine "b) Kostenerhöhung" to "sourceCode" indentation 2
  addCodeLine "Berechnung der Transportkostenerhöhung w_ij (zu Kunde j) bei " to "sourceCode" indentation 2
  addCodeLine "Schließung von Lager i in I:" to "sourceCode" indentation 2
  addCodeLine "w_ij = c_i2 - c_i1 , falls i = i1 (Differenz der geringsten und zweitgeringsten Kosten)" to "sourceCode" indentation 3
  addCodeLine "w_ij = 0           , sonst" to "sourceCode" indentation 3
  addCodeLine "Bereche Transportkostenerhöhung bei Schließung des Lagers i" to "sourceCode" indentation 2
  addCodeLine "E_i = Summe_i (w_ij) für alle i aus I" to "sourceCode" indentation 3
  addCodeLine "c) Kostenersparniss" to "sourceCode" indentation 2
  addCodeLine "Berechne Gesamtkostenersparniss d_i bei Schließung des Lagers i" to "sourceCode" indentation 2
  addCodeLine "d_i = f_i - E_i    für alle nicht geschlossen Lager" to "sourceCode" indentation 3
  addCodeLine " Falls d_i <= 0 dann bleibt Lager i immer offen" to "sourceCode" indentation 3
  addCodeLine "d) Schließen" to "sourceCode" indentation 2
  addCodeLine "Schließung des Lagers k mit der größten Einsparung" to "sourceCode" indentation 2
  addCodeLine "k = argmax {d_i} für die gilt d_i > 0 und i in I" to "sourceCode" indentation 3
  addCodeLine "I := I /k" to "sourceCode" indentation 3
  addCodeLine "e) Abbruch?" to "sourceCode" indentation 2
  addCodeLine "falls d_i <= 0 für alle i aus I" to "sourceCode" indentation 3
  text "infoText1" "1" offset (0, 440) from "dropCost_Header" NW color (0, 0, 0) depth 1 font SansSerif size 12
  text "infoText2" "2" offset (0, 10) from "infoText1" SW color (0, 0, 0) depth 1 font SansSerif size 12
  text "dropCost_TableHead_0" "c_ij*r_j" offset (10, 0) from "dropCost_Header" SW color (0, 0, 0) depth 1 font SansSerif size 12
  hide "dropCost_into_Header" "dropCost_into_Header2" "dropCost_introProblem1" "dropCost_introProblem2" "dropCost_introProblem3" "dropCost_introProblem4" "dropCost_introProblem5" "dropCost_introIdea1" "dropCost_introIdea2" "dropCost_introIdea3" "dropCost_introIdea4" "dropCost_introIdea5" "dropCost_introIdea6" "dropCost_introIdea7" "dropCost_introIdea8" "dropCost_introIdea9" "dropCost_introIdea10" "backgroundRectInto" 
