<?xml version="1.0" encoding="ISO-8859-1"?>
<PropertiesTreeModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="PropertiesTreeModel.xsd">
	<Folder name="Root">
		<Primitive type="String">
			<name>clauseSet</name>
			<value><String>{!s, q}, {!q, p}, {!q, !p}, {!s, !q}</String></value>
			<description>Input for resolution</description>
		</Primitive>
		<Primitive type="Color">
			<name>clauseHighlightColor1</name>
			<value><Color>(0, 127, 255)</Color></value>
			<description>Color for highlighting clauses</description>
		</Primitive>
		<Primitive type="Color">
			<name>clauseHighlightColor2</name>
			<value><Color>(255, 216, 31)</Color></value>
			<description>Color for highlighting clauses and specific cells</description>
		</Primitive>
		<Primitive type="Color">
			<name>clauseCheckColor</name>
			<value><Color>(255, 66, 31)</Color></value>
			<description>Color when checking a clause for unsatisfiability</description>
		</Primitive>
		<Primitive type="boolean">
			<name>showQuestions</name>
			<value><boolean>true</boolean></value>
			<description>Enables questions</description>
		</Primitive>
	</Folder>
</PropertiesTreeModel>